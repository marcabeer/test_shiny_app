[{"name":"app.R","content":"library(shiny)\r\nlibrary(bslib)\r\nlibrary(raster)\r\nlibrary(ggplot2)\r\n\r\n#r = raster(\"./data/r1.tif\")\r\n\r\n# ui <- basicPage(\r\n#   \r\n#   # output the raster\r\n#   plotOutput(outputId = \"plot1\")\r\n#   \r\n# )\r\n\r\n# sidebarPanel(titlePanel(\"Select scenario\"),\r\n#              width = 4)\r\n# \r\n# mainPanel(titlePanel(\"Map\"),\r\n#           plotOutput(outputId = \"plot1\"),\r\n#           width = 8)\r\n\r\nui = fluidPage(\r\n  titlePanel(fluidRow(column(width=4, offset=4, \"Data explorer\"))),\r\n  sidebarPanel(h1(\"Options\"),\r\n               selectInput(\"data\", \"Select dataset\", choices = c(\"Raster 1\", \"Raster 2\")),\r\n               selectInput(\"color\", \"Select color scheme\", choices = c(\"viridis\", \"mako\", \"magma\")),\r\n               width = 4),\r\n  \r\n  mainPanel(plotOutput(outputId = \"plot1\"),\r\n            width = 8),\r\n  position = \"left\",\r\n  fluid = TRUE\r\n)\r\n\r\nserver <- function(input, output) {\r\n  \r\n  # convert raster to df for plotting\r\n  ## probably want to do this in advance for large rasters\r\n  ## and load in R object\r\n  # r_df = as.data.frame(r, xy=TRUE)\r\n  # colnames(r_df) = c(\"x\", \"y\", \"z\")\r\n  # \r\n  # \r\n  # output plot\r\n  \r\n  ## note that several options are reactive\r\n  # dataset\r\n  # fill color scheme\r\n  output$plot1 = renderPlot({\r\n    \r\n    if (input$data == \"Raster 1\"){\r\n      r = raster(\"./data/r1.tif\")\r\n    }\r\n    if (input$data == \"Raster 2\"){\r\n      r = raster(\"./data/r2.tif\")\r\n    }\r\n    \r\n    # create df from raster for plotting\r\n    r_df = as.data.frame(r, xy=TRUE)\r\n    colnames(r_df) = c(\"x\", \"y\", \"z\")\r\n    \r\n    # create plot\r\n    ggplot(data=r_df, mapping=aes(x=x, y=y, fill=z))+\r\n    geom_tile()+\r\n    scale_fill_viridis_c(option=input$color)+\r\n    labs(x=\"Longitude\", y=\"Latitude\", fill=\"Env\")+\r\n    coord_equal()\r\n  },\r\n  res=96\r\n  )\r\n  \r\n  # output$plot1 = renderPlot({\r\n  #   ggplot(data=data.frame(x=1:10, y=21:30), mapping=aes(x=x, y=y))+\r\n  #     geom_point()\r\n  # }, res=96)\r\n  \r\n}\r\n\r\nshinyApp(ui = ui, server = server)\r\n","type":"text"},{"name":"data/r1.tif","content":"SUkqAAgAAAAQAAABAwABAAAANAAAAAEBAwABAAAANAAAAAIBAwABAAAAIAAAAAMBAwABAAAA\nBQAAAAYBAwABAAAAAQAAABEBBAACAAAA1gAAABUBAwABAAAAAQAAABYBAwABAAAAJwAAABcB\nBAACAAAAzgAAABwBAwABAAAAAQAAAD0BAwABAAAAAQAAAFMBAwABAAAAAwAAAA6DDAADAAAA\n6AEAAIKEDAAGAAAAAAIAAICkAgAAAQAA3gAAAIGkAgAKAAAA3gEAAAAAAACvCwAAWgQAADAC\nAADfDQAAPEdEQUxNZXRhZGF0YT4KICA8SXRlbSBuYW1lPSJTVEFUSVNUSUNTX01BWElNVU0i\nIHNhbXBsZT0iMCI+MTwvSXRlbT4KICA8SXRlbSBuYW1lPSJTVEFUSVNUSUNTX01FQU4iIHNh\nbXBsZT0iMCI+MS4jU05BTjwvSXRlbT4KICA8SXRlbSBuYW1lPSJTVEFUSVNUSUNTX01JTklN\nVU0iIHNhbXBsZT0iMCI+MDwvSXRlbT4KICA8SXRlbSBuYW1lPSJTVEFUSVNUSUNTX1NURERF\nViIgc2FtcGxlPSIwIj4xLiNTTkFOPC9JdGVtPgo8L0dEQUxNZXRhZGF0YT4KAC0zLjRlKzAz\nOAAAAAAAAADwPwAAAAAAAPA/AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA\nAAAAAAAAAAAAAEpAAAAAAAAAAACAI1GIwfJdLJYfKFQKAfINBIIfPF4PAewpQD2JRRkshkRW\nFwVLBYfsRhkMftNpNIfKtVCofrpcjkfvZ6nUfthrlcfxskD+DSKFCCdxyEwseRI4Rh4UmMj2\nDQimwIGRGlw2H1YBD9JJEI1quD+pD9OJsN0OeySTOJwmEfohDgcf0isXGqxYfN1uNwfL9fD6\nXzEf3guD+WS6YTJzuZzD6x2W+X7C2BGAwfqZSiWhsiVqpVUWGVLPDyLD3QTidLBXi8f48fs5\nmk0f2q2TSbUibznJZS+X2nj6eXduFwfSiVbsfYnF0iPCDl8fFD6BQTe3i9Pt9PofLVaDSvV3\nBbG1j+HVne8at12Mj7z6u++y/UHA8H44PbbKRyW/zLI6jVXbtBoooQKsiCgo8i67OIzbOwSt\ny4Nc2B2HWNaTmkKTKsu1rXh+/7cropKgt8ZAkOyWhaOgyaqDhEkALkpsCtGwQfOs7DLMwyL7\nP49i+vHCkLMinjwNmmsgh+xIzJnIb7NtCMJu/DiUR8zkTqm5UXKMhyIKa37tRM6KQJEjbNRr\nIpzSO2j5h/JizR7DzhpTFcBynELNS80cwh7GMsKoeD1K/LkZOuH0ctNDrvzVKE0wkH7rD1NE\n1QfNFIUZDzbp1IDWJ+8RBKy407I4Hs9N/ECWM7Bq6T5U0cyA21CRzHK0O7HUQxG5AfVNGLRt\nBFqJh7McZ1nWL4Uyw8iVg+rwyXRVJzVHLIyNMkj0Q1la1a1LMuAvU/wTWs6o++DRv/PbNpc6\njGLJSt0VM1kx1NBNCPtQla3DcESrm79DUVSE1NtVjcURM9jrZSdIXc3FnyfCsyMXBNEVda1i\nPXHMeLk0EvN7Xc+LDWFHw1YliMjDlYTDcyy1hQj1wSu1SS+H8czG+1iWe+19qXRZ9Ubfma5j\nMtKTHRCwxzREeR5NVCSBQkEyBIFyyrXjjVNIFYUg79YV/kaw3LkcEx/EU